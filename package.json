    "react-hook-form": "^7.48.2",
    "sonner": "^1.2.4",
    "tailwind-variants": "^0.1.18",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "whatwg-fetch": "^3.6.19",
    "zod": "^3.22.4"
  },
