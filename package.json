    "@react-aria/visually-hidden": "^3.8.7",
    "@trpc/client": "^11.0.0-rc.382",
    "@trpc/server": "^11.0.0-rc.382",
    "@types/node": "20.10.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@vercel/analytics": "^1.1.1",
    "async-retry": "^1.3.3",
    "autoprefixer": "10.4.16",
    "clsx": "^2.0.0",
