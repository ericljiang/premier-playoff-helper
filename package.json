{
  "name": "next-app-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -H local.example.com -p 80",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "updateAgentIcons": "curl 'https://valorant-api.com/v1/agents?isPlayableCharacter=true' | jq '{agentIcons: [.data | .[] | {displayName, displayIcon}]}' > resources/agent-icons.json",
    "updateMaps": "curl 'https://valorant-api.com/v1/maps' | jq '{maps: [.data | .[] | {displayName, displayIcon, xMultiplier, xScalarToAdd, yMultiplier, yScalarToAdd}]}' > resources/maps.json",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@extendslcc/trpc-panel": "^1.3.6",
